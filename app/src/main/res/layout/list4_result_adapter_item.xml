<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/drawing_time"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:gravity="center"
        android:minEms="@integer/analyze_list4_result_drawing_time" />

    <TextView
        android:id="@+id/drawing_number"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:gravity="center"
        android:minEms="@integer/analyze_list4_result_drawing_number" />

    <TextView
        android:id="@+id/odd_and_plural"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:gravity="center"
        android:minEms="@integer/analyze_list4_result_odd_and_plural" />

    <TextView
        android:id="@+id/mod3"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:gravity="center"
        android:minEms="@integer/analyze_list4_result_mod_3" />

    <TextView
        android:id="@+id/sum"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:gravity="center"
        android:minEms="@integer/analyze_list4_result_sum" />

    <TextView
        android:id="@+id/average"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:gravity="center"
        android:minEms="@integer/analyze_list4_result_average" />

    <TextView
        android:id="@+id/number_of_odd_and_plural"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:gravity="center"
        android:minEms="@integer/analyze_list4_result_number_of_odd_and_plural" />

</LinearLayout>